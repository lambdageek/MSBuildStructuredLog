{
  "compilerOptions": {
      "rootDir": "./src",
      "skipLibCheck": true,
      "module": "Node16",
      "moduleResolution": "Node16",
      "outDir": "./out",
      "emitDeclarationOnly": true,
      "strict": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "declaration": true,
      "stripInternal": true,
      "sourceMap": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true
  }
}
